{                                              
    "extends": "default",                      
    "rules": {                                 
        "indent": ["error", 4],                
        "quotes": ["error", "double"],         
        "max-line-length": ["error", 150],
        "contract-name-camelcase": false,
        "code-complexity": ["error",17],
        "function-max-lines":["warn"],
        "max-states-count":["error",36],
        "check-send-result":["warn"],
        "multiple-sends": ["warn"]
    }                                          
}                                              